<?xml version="1.0" encoding="utf-8"?>


<RelativeLayout
        android:id="@+id/cityid"
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Dashboard.Dashboard"
        android:background="#225883">

<!--    <fragment android:id="@+id/myMap"-->
<!--              android:layout_width="match_parent"-->
<!--              android:layout_height="match_parent"-->
<!--              android:name="com.google.android.gms.maps.SupportMapFragment"/>-->


    <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            app:title="@string/app_name"
            app:titleTextColor="@color/white"
            android:background="@color/purple_700"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"/>

    <ImageView
            android:id="@+id/log_out"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/ic_baseline_logout_24"
            android:layout_marginLeft="370dp"
            android:layout_marginTop="10dp"/>

    <ImageView
            android:id="@+id/location"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/lctn"
            android:layout_marginLeft="320dp"
            android:layout_marginTop="10dp"/>

    <ImageView
            android:id="@+id/profile"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/person"
            android:layout_marginLeft="260dp"
            android:layout_marginTop="10dp"/>

    <pl.droidsonroids.gif.GifImageView
            android:id="@+id/go"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@drawable/go_explore"
            android:layout_marginTop="-340dp"
    />


    <ImageView android:layout_width="70dp"
               android:layout_height="70dp"
               android:id="@+id/history"
               android:src="@drawable/history"
               android:layout_marginLeft="30dp"
               android:layout_marginTop="360dp"/>

    <TextView android:layout_width="70dp"
              android:layout_height="70dp"
              android:layout_marginLeft="40dp"
              android:layout_marginTop="430dp"
              android:text="History"
              android:textColor="@color/white"
              android:textStyle="bold"
    />

    <ImageView android:layout_width="70dp"
               android:layout_height="70dp"
               android:id="@+id/pmpml"
               android:layout_marginLeft="170dp"
               android:src="@drawable/pmpml"
               android:layout_marginTop="360dp"
    />
    <TextView android:layout_width="70dp"
              android:layout_height="70dp"
              android:layout_marginLeft="180dp"
              android:layout_marginTop="430dp"
              android:text="PMPML"
              android:textColor="@color/white"
              android:textStyle="bold"
    />

    <ImageView android:layout_width="70dp"
               android:layout_height="70dp"
               android:id="@+id/hospitals"
               android:src="@drawable/hospital"
               android:layout_marginLeft="310dp"
               android:layout_marginTop="360dp"/>
    <TextView android:layout_width="70dp"
              android:layout_height="70dp"
              android:layout_marginLeft="312dp"
              android:layout_marginTop="430dp"
              android:text="Hospitals"
              android:textColor="@color/white"
              android:textStyle="bold"
    />

    <ImageView android:layout_width="70dp"
               android:layout_height="70dp"
               android:id="@+id/cafe"
               android:layout_marginLeft="30dp"
               android:src="@drawable/cafe"
               android:layout_marginTop="470dp"/>

    <TextView android:layout_width="70dp"
              android:layout_height="70dp"
              android:layout_marginLeft="50dp"
              android:layout_marginTop="535dp"
              android:text="Cafe"
              android:textColor="@color/white"
              android:textStyle="bold"
    />

    <ImageView android:layout_width="70dp"
               android:layout_height="70dp"
               android:id="@+id/temple"
               android:layout_marginLeft="170dp"
               android:src="@drawable/temple"
               android:layout_marginTop="470dp"/>
    <TextView android:layout_width="70dp"
              android:layout_height="70dp"
              android:layout_marginLeft="180dp"
              android:layout_marginTop="535dp"
              android:text="Temple"
              android:textColor="@color/white"
              android:textStyle="bold"
    />

    <ImageView android:layout_width="70dp"
               android:layout_height="70dp"
               android:id="@+id/hotel"
               android:layout_marginLeft="310dp"
               android:src="@drawable/food"
               android:layout_marginTop="470dp"/>
    <TextView android:layout_width="70dp"
              android:layout_height="70dp"
              android:layout_marginLeft="330dp"
              android:layout_marginTop="530dp"
              android:text="Hotels"
              android:textColor="@color/white"
              android:textStyle="bold"
    />

    <ImageView android:layout_width="70dp"
               android:layout_height="70dp"
               android:id="@+id/places"
               android:layout_marginLeft="170dp"
               android:src="@drawable/place"
               android:layout_marginTop="580dp"/>

    <TextView android:layout_width="70dp"
              android:layout_height="70dp"
              android:layout_marginLeft="180dp"
              android:layout_marginTop="650dp"
              android:text="Places"
              android:textColor="@color/white"
              android:textStyle="bold"
    />


</RelativeLayout>
